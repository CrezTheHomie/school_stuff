#include <iostream>
#include <cstdlib>
#include <limits>
#include <cmath>
#include <iomanip>
#include <string>
#include <sstream>


using namespace std;

/* all numbers are doubles. */

int main(){

	string input;
	getline (cin,input);
	stringstream stream;
	stream.str(input);
	double n;
	char op;

	while(stream >> n || stream >> op){
		cout << n << op << "\n";
	}




	return 0;
}
